{
  "expo": {
    "name": "dialogue-mobile",
    "slug": "dialogue-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dialogue-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dialogue-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dialogue-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow Dialogue to access your Face ID biometric data."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/allDarkDialogueLogo.png",
          "dark": {
            "image": "./assets/images/allLightDialogueLogo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "enableFullScreenImage_legacy": false
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
